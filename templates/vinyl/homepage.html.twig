{% extends 'base.html.twig' %}

{% block title %}Create a new record | {{ parent() }}{% endblock %}
{% block body %}
<h1>{{ title }}</h1>

<div>
    Tracks:
    <ul>
    {% for song in songs %}
    <li>
        {{ song.song}} - {{ song.artist }}
    </li>
    {% endfor %}
    </ul>
</div>
{% endblock %}
